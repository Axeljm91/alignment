      </propertyDefinition>
      <propertyDefinition>
         <name>Inserted</name>
         <type>BOOLEAN</type>
         <indexed>false</indexed>
         <writable>false</writable>
         <description>Inserted status from the ECU/BTCU</description>
      </propertyDefinition>
      <propertyDefinition>
         <name>Retracted</name>
         <type>BOOLEAN</type>
         <indexed>false</indexed>
         <writable>false</writable>
         <description>Retracted status from the ECU/BTCU</description>
      </propertyDefinition>
      <propertyDefinition>
         <name>InsertionStatus</name>
